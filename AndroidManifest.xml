<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cse110.makingbank"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <!-- This app requires permission to access the Internet -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:name=".MakingBank"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginPage"
            android:exported="true"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminHomePage">
        </activity>
        <activity
            android:name=".CreateAccountPage">
        </activity>
        <activity
            android:name=".BankHomePage">
        </activity>
        <activity
            android:name=".ViewAccountPage">
        </activity>
        <activity
            android:name=".AccountOptions">
        </activity>
        <activity
            android:name=".PasswordReset">
        </activity>
        <activity
            android:name=".MyInfo">
        </activity>
    </application>

</manifest>
